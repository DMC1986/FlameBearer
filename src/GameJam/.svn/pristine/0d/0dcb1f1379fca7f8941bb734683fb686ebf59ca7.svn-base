                        <auto-mirrorflip>(none)</auto-mirrorflip>
                    </properties>
                    <behavior-instances>
                        <behavior-instance type="CustomMovement">
                            <properties>
                                <stepping-mode>Linear</stepping-mode>
                                <pixels-per-step>5</pixels-per-step>
                            </properties>
                        </behavior-instance>
                    </behavior-instances>
                    <world>
                        <x>522</x>
                        <y>306</y>
                        <z>0</z>
                        <width>30</width>
                        <height>50</height>
                        <depth>0</depth>
                        <angle>0</angle>
                        <opacity>0.5</opacity>
                    </world>
                </instance>
            </instances>
        </layer>
        <layer name="Layer 3">
            <initially-visible>1</initially-visible>
            <background-color>233,233,255</background-color>
            <transparent>0</transparent>
            <parallax>
                <x>1</x>
