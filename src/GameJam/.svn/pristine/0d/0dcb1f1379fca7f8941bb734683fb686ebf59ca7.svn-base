                    <world>
                        <x>1357.770751953</x>
                        <y>493.229248047</y>
                        <z>0</z>
                        <width>129.541503906</width>
                        <height>129.541503906</height>
                        <depth>0</depth>
                        <angle>0</angle>
                        <opacity>1</opacity>
                    </world>
